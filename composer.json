{
    "name": "carropublic/discussion",
    "description": "Discussion like comments",
    "license": "MIT",
    "authors": [
        {
            "name": "Universe",
            "email": "universe@carro.co",
            "homepage": "https://carro.sg"
        }
    ],
    "homepage": "https://github.com/carropublic/discussion",
    "keywords": ["Laravel", "Discussion"],
    "require": {
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|^8.0|^10.1",
        "sempro/phpunit-pretty-print": "^1.2",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "CarroPublic\\Discussion\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CarroPublic\\Discussion\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CarroPublic\\Discussion\\DiscussionServiceProvider"
            ],
            "aliases": {
                "Discussion": "CarroPublic\\Discussion\\Facades\\Discussion"
            }
        }
    }
}
